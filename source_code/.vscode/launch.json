  "version": "0.2.0",
  "configurations": [
    {
      "name": "Cortex Debug",
      "cwd": "${workspaceRoot}",
      "executable": "${workspaceFolder}/build/build/stm32f4.elf",
      "device": "stm32f407vg",
      "request": "launch",
      "type": "cortex-debug",
      "servertype": "jlink",
      "interface": "swd",
      "ipAddress": null,
      "serialNumber": null,
      "svdFile": "${workspaceFolder}/../util/STM32F407.svd",
      "runToMain": true,
      "armToolchainPath": "/opt/gcc-arm-none-eabi-8.3.1/bin"
    }
  ]
}